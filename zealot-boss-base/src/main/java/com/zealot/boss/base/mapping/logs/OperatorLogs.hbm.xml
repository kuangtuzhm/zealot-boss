<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.zealot.boss.base.entity.logs.OperatorLogs" table="operatorLogs" dynamic-update="true">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="uid" column="uid" type="java.lang.Integer"/>
        <property name="operObject" column="operObject" type="java.lang.Integer"/>
        <property name="operatorType" column="operatorType" type="java.lang.Integer"/>
        <property name="operAction" column="operAction" type="java.lang.Integer"/>
        <property name="operatorDesc" column="operatorDesc" type="java.lang.String"/>
        <property name="ip" column="ip" type="java.lang.String"/>
        <property name="createTime" column="createTime" type="java.lang.String"/>
    </class>
</hibernate-mapping>
