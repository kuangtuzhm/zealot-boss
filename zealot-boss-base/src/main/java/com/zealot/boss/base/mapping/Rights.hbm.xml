<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.zealot.boss.base.entity.Rights" table="rights">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="rightCode" column="right_code" type="java.lang.String"/>
        <property name="parentCode" column="parentCode" type="java.lang.String"/>
        <property name="rightDesc" column="right_desc" type="java.lang.String"/>
        <property name="uri" column="uri" type="java.lang.String"/>
        <property name="state" column="state" type="java.lang.Integer"/>
        <property name="iorder" column="iorder" type="java.lang.Integer"/>
        <property name="type" column="type" type="java.lang.Integer"/>
        <property name="createdBy" column="createdBy" type="java.lang.Integer"/>
        <property name="updatedBy" column="updatedBy" type="java.lang.Integer"/>
        <property name="createTime" column="createTime" type="java.lang.String"/>
        <property name="updateTime" column="updateTime" type="java.lang.String"/>
    </class>
</hibernate-mapping>
