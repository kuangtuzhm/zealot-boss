<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.zealot.boss.base.entity.authorize.SimpleRights" table="rights"> 
        <id name="rightCode" type="java.lang.String">
            <column name="right_code" />
            <generator class="assigned" />
        </id>
        <property name="parentCode" column="parent_code" type="java.lang.String"/>
        <property name="sysCode" column="sys_code" type="java.lang.String"/>
        <property name="rightDesc" column="right_desc" type="java.lang.String"/>
        <property name="type" column="type" type="java.lang.Integer"/>
        <property name="iorder" column="iorder" type="java.lang.Integer"/>
    </class>
</hibernate-mapping>
