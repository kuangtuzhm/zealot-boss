<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.zealot.boss.base.entity.authorize.Department" table="department" dynamic-update="true">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="name" column="name" type="java.lang.String"/>
        <property name="parentId" column="parent_id" type="java.lang.Integer"/>
        <property name="path" column="path" type="java.lang.String"/>
        <property name="grade" column="grade" type="java.lang.Integer"/>
        <property name="iorder" column="iorder" type="java.lang.Integer"/>
        <property name="createdBy" column="createdBy" type="java.lang.Integer"/>
        <property name="updatedBy" column="updatedBy" type="java.lang.Integer"/>
        <property name="createTime" column="createTime" type="java.util.Date"/>
        <property name="updateTime" column="updateTime" type="java.util.Date"/>
    </class>
</hibernate-mapping>
